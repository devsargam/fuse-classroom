{
  "$schema": "https://turbo.build/schema.json",
  "globalDependencies": ["**/.env.*local"],
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": [".next/**", "!.next/cache/**"],
      "env": ["GITHUB_ID", "GITHUB_SECRET", "NEXTAUTH_URL", "NEXTAUTH_SECRET"]
    },
    "lint": {
      "dependsOn": ["^lint"]
    },
    "dev": {

      "cache": false,
      "persistent": true
    },
    "db:generate": {
      "cache": false
    },
    "db:push": {
      "cache": false
    },
    "db:studio": {
      "cache": false
    }
  }
}
